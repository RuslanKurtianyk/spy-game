    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^18.15.11",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
